{
    "name": "datalinx/omnipay-bankart",
    "type": "library",
    "description": "Bankart v2 (2020) driver for the Omnipay payment processing library",
    "keywords": [
        "bankart",
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment"
    ],
    "homepage": "https://github.com/datalinx/omnipay-bankart",
    "license": "MIT",
    "authors": [
        {
            "name": "Omer Sabic",
            "email": "omer.sabic@gmail.com"
        },
        {
            "name": "Omnipay Bankart Contributors",
            "homepage": "https://github.com/datalinx/omnipay-bankart/contributors"
        }
    ],
    "autoload": {
        "psr-4": { "Omnipay\\Bankart\\" : "src/" }
    },
    "require": {
        "omnipay/common": "~2.0",
        "datalinx/pgc-php-client": "@dev"
    },
    "require-dev": {
        "omnipay/tests": "~2.0",
        "vlucas/phpdotenv": "^2.4"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/datalinx/PHPPaymentGateway"
        }
    ]
}
